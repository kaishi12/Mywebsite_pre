@model IEnumerable<MyWebsite.ViewModels.Home.Slides.Slides>

@foreach (var item in ViewBag.Slide)
{
    <div class="owl__item">
        <a href="/manga/@item.Alias-@item.MangaId/@item.DefaultLang">
            <div class="item">
                <img src="~/Cover/@item.CoverLink" onError="this.onerror=null;this.src='/RootPicture/error-404.jpg';" />
                <h2>@item.FullName</h2>
            </div>
        </a>
    </div>
}
